bool is_prime(int x) {
	if (x < 2 || x % 2 == 0) return false;
	for (int i = 3; i*i <= x; i += 2) {
		if (x % i == 0) return false;
	}
	return true;
}

vector<int> primes_up_to(int x) {
	vector<bool> sieve;
	vector<int> primes;
	for (int i = 2; i <= x; i++) {
		if (sieve[i]) continue;
		primes.push_back(i);
		for (int j = i * 2; j <= n; j += i) {
			sieve[j] = 1;
		}
	}
	return primes;
}
