#define fori(x) for (int i = 0; i < (x); i++)
#define forj(x) for (int j = 0; j < (x); j++)
#define pii pair<int, int>
#define pll pair<ll, ll>
#define vec vector
#define minpq priority_queue<int, vector<int>, greater<int>>
#define debug(x) cout << #x << " = " << x << endl;
#define indexed_set tree<int, null_type, less<int>, rb_tree_tag, tree_order_statistics_node_update>
#define fastio() cin.tie(0)->sync_with_stdio(0)
#define printx(x) do { cout << (x) << '\n'; exit(0); } while (0)
#define inputvector(x, n) x.resize(n); forj(n) cin >> x[j]
#define hashtable gp_hash_table
#define all(x) x.begin(), x.end()
#define rall(x) x.rbegin(), x.rend()
#define sz(x) (int)(x).size()
#define pb push_back
#define mp make_pair

#ifndef DBG
#define usaco(pid) freopen((string(pid) + ".in").c_str(), "r", stdin); freopen((string(pid) + ".out").c_str(), "w", stdout)
#else
#define usaco(pid)
#endif
